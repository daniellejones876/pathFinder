<h1>
Hi from the log index!
</h1>

<ul>
<% @logbooks.each do |logbook| %>
<li>
  Summary: <%= logbook.summary %>
</li>
<li>
  <%= logbook.created_at.strftime("%d of %B, %Y") %>, <%= logbook.created_at.strftime("%I:%M %p") %>
</li>
<li>
<%= link_to "Open", logbook_path(logbook) %>
</li>
<% end %>
</ul>

<%= link_to "Homepage", home_path %>
<%= link_to "New log", new_logbook_path %>
